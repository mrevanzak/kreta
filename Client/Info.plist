<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>app.kreta.kreta</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>kreta</string>
			</array>
		</dict>
	</array>
	<key>CONVEX_URL</key>
	<string>https://$(CONVEX_URL)</string>
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>instagram-stories</string>
	</array>
	<key>NSAlarmKitUsageDescription</key>
	<string>Kreta uses AlarmKit to alert you 10 minutes before arriving at your destination station</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Kreta menggunakan lokasi Anda untuk memberitahu saat mendekati stasiun kereta</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Kreta menggunakan lokasi Anda di latar belakang untuk memberitahu saat mendekati stasiun kereta, bahkan saat aplikasi tidak sedang digunakan</string>
	<key>POSTHOG_API_KEY</key>
	<string>$(POSTHOG_API_KEY)</string>
	<key>SENTRY_DSN</key>
	<string>https://$(SENTRY_DSN)</string>
	<key>UIBackgroundModes</key>
	<array>
		<string>location</string>
		<string>remote-notification</string>
	</array>
</dict>
</plist>
